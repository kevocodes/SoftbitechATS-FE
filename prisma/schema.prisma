generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  email     String  @unique
  password  String
  name      String
  lastname  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@map("users")
}

model Technology {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  image      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@map("technologies")
}